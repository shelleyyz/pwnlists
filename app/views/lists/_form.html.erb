<%= form_for @list do |f| %>

  <%= f.label :name, :autofocus => true%>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_field :description %>

<%# create ability for user to add game to list , add ids to joining table %>

  <%= f.label "Games" %>
  <%= f.collection_check_boxes :game_ids, Game.all, :id, :title %>
<%= f.submit %>

<% end %>
