<%= form_for @user do |f| %>
<%= f.label :first_name %>
<%= f.text_field :first_name %>

<%= f.label :last_name %>
<%= f.text_field :last_name %>

<%= f.label :email %>
<%= f.text_field :email %>

<%= f.label :bio %>
<%= f.text_field :bio %>

<%= f.label :city %>
<%= f.text_field :country %>

<%= f.label :image %>
<%= f.url_field :image %>

<%= f.submit %>
<% end %>
