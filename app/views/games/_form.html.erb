<%= form_for @game do |f| %>

  <%= f.label :title, :autofocus => true%>
  <%= f.text_field :title %>

  <%= f.label :genre %>
  <%= f.text_field :genre %>

  <%= f.label :developer %>
  <%= f.text_field :developer %>

  <%= f.label :image %>
  <%= f.url_field :image %>

  <%= f.submit %>

<% end %>
